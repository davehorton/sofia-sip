Makefile.in:# note: when glib devel files are not available, make should not
Makefile.in:# This directory's subdirectories are mostly independent; you can cd
Makefile.in:# To change the values of `make' variables: instead of editing Makefiles,
Makefile.in:# (1) if the variable is set in `config.status', edit `config.status'
Makefile.in:	target=`echo $@ | sed s/-recursive//`; \
Makefile.in:	  echo "Making $$target in $$subdir"; \
Makefile.in:	    local_target="$$target-am"; \
Makefile.in:	    local_target="$$target"; \
Makefile.in:	  ($(am__cd) $$subdir && $(MAKE) $(AM_MAKEFLAGS) $$local_target) \
Makefile.in:	  $(MAKE) $(AM_MAKEFLAGS) "$$target-am" || exit 1; \
Makefile.in:	target=`echo $@ | sed s/-recursive//`; \
Makefile.in:	  echo "Making $$target in $$subdir"; \
Makefile.in:	    local_target="$$target-am"; \
Makefile.in:	    local_target="$$target"; \
Makefile.in:	  ($(am__cd) $$subdir && $(MAKE) $(AM_MAKEFLAGS) $$local_target) \
Makefile.in:	tardir=$(distdir) && $(am__tar) | GZIP=$(GZIP_ENV) gzip -c >$(distdir).tar.gz
Makefile.in:	tardir=$(distdir) && $(am__tar) | bzip2 -9 -c >$(distdir).tar.bz2
Makefile.in:	tardir=$(distdir) && $(am__tar) | lzma -9 -c >$(distdir).tar.lzma
Makefile.in:	tardir=$(distdir) && $(am__tar) | xz -c >$(distdir).tar.xz
Makefile.in:dist-tarZ: distdir
Makefile.in:	tardir=$(distdir) && $(am__tar) | compress -c >$(distdir).tar.Z
Makefile.in:dist-shar: distdir
Makefile.in:	shar $(distdir) | GZIP=$(GZIP_ENV) gzip -c >$(distdir).shar.gz
Makefile.in:	tardir=$(distdir) && $(am__tar) | GZIP=$(GZIP_ENV) gzip -c >$(distdir).tar.gz
Makefile.in:# This target untars the dist file and tries a VPATH configuration.  Then
Makefile.in:# it guarantees that the distribution is self-contained by making another
Makefile.in:# tarfile.
Makefile.in:	*.tar.gz*) \
Makefile.in:	  GZIP=$(GZIP_ENV) gunzip -c $(distdir).tar.gz | $(am__untar) ;;\
Makefile.in:	*.tar.bz2*) \
Makefile.in:	  bunzip2 -c $(distdir).tar.bz2 | $(am__untar) ;;\
Makefile.in:	*.tar.lzma*) \
Makefile.in:	  unlzma -c $(distdir).tar.lzma | $(am__untar) ;;\
Makefile.in:	*.tar.xz*) \
Makefile.in:	  xz -dc $(distdir).tar.xz | $(am__untar) ;;\
Makefile.in:	*.tar.Z*) \
Makefile.in:	  uncompress -c $(distdir).tar.Z | $(am__untar) ;;\
Makefile.in:	*.shar.gz*) \
Makefile.in:	  GZIP=$(GZIP_ENV) gunzip -c $(distdir).shar.gz | unshar ;;\
Makefile.in:	@(echo "$(distdir) archives ready for distribution: "; \
Makefile.in:	dist-bzip2 dist-gzip dist-lzma dist-shar dist-tarZ dist-xz \
Makefile.in:@HAVE_DOXYGEN_TRUE@	  fgrep -v 'Warning: explicit' ;} | { read x; exit $$x ;}
Makefile.in:@HAVE_LCOV_TRUE@	-find . -name "*.gcda" | xargs rm -rf
Makefile.in:@HAVE_LCOV_TRUE@	find . -name "*.gcda" | xargs rm -f || true
Makefile.in:@HAVE_LCOV_TRUE@	find . -name "*.gcda" | xargs rm -f || true
Makefile.in:@HAVE_LCOV_TRUE@	rsync -rvz -e ssh --delete lcov/* sofia-sip.org:/var/www/coverage/lcov/
