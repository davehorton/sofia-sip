aclocal.m4:# Check how to create a tarball.                            -*- Autoconf -*-
aclocal.m4:# Copyright (C) 2004, 2005  Free Software Foundation, Inc.
aclocal.m4:# This file is free software; the Free Software Foundation
aclocal.m4:# Check how to create a tarball in format FORMAT.
aclocal.m4:# FORMAT should be one of `v7', `ustar', or `pax'.
aclocal.m4:# Substitute a variable $(am__tar) that is a command
aclocal.m4:# writing to stdout a FORMAT-tarball containing the directory
aclocal.m4:# $tardir.
aclocal.m4:#     tardir=directory && $(am__tar) > result.tar
aclocal.m4:# Substitute a variable $(am__untar) that extract such
aclocal.m4:# a tarball read from stdin.
aclocal.m4:#     $(am__untar) < result.tar
aclocal.m4:[# Always define AMTAR for backward compatibility.
aclocal.m4:AM_MISSING_PROG([AMTAR], [tar])
aclocal.m4:     [am__tar='${AMTAR} chof - "$$tardir"'; am__untar='${AMTAR} xf -'],
aclocal.m4:     [m4_case([$1], [ustar],, [pax],,
aclocal.m4:              [m4_fatal([Unknown tar format])])
aclocal.m4:AC_MSG_CHECKING([how to create a $1 tar archive])
aclocal.m4:# Loop over all known methods to create a tar archive until one works.
aclocal.m4:_am_tools='gnutar m4_if([$1], [ustar], [plaintar]) pax cpio none'
aclocal.m4:_am_tools=${am_cv_prog_tar_$1-$_am_tools}
aclocal.m4:# Solaris sh will not grok spaces in the rhs of `-'.
aclocal.m4:  gnutar)
aclocal.m4:    for _am_tar in tar gnutar gtar;
aclocal.m4:      AM_RUN_LOG([$_am_tar --version]) && break
aclocal.m4:    am__tar="$_am_tar --format=m4_if([$1], [pax], [posix], [$1]) -chf - "'"$$tardir"'
aclocal.m4:    am__tar_="$_am_tar --format=m4_if([$1], [pax], [posix], [$1]) -chf - "'"$tardir"'
aclocal.m4:    am__untar="$_am_tar -xf -"
aclocal.m4:  plaintar)
aclocal.m4:    # Must skip GNU tar: if it does not support --format= it doesn't create
aclocal.m4:    # ustar tarball either.
aclocal.m4:    (tar --version) >/dev/null 2>&1 && continue
aclocal.m4:    am__tar='tar chf - "$$tardir"'
aclocal.m4:    am__tar_='tar chf - "$tardir"'
aclocal.m4:    am__untar='tar xf -'
aclocal.m4:    am__tar='pax -L -x $1 -w "$$tardir"'
aclocal.m4:    am__tar_='pax -L -x $1 -w "$tardir"'
aclocal.m4:    am__untar='pax -r'
aclocal.m4:    am__tar='find "$$tardir" -print | cpio -o -H $1 -L'
aclocal.m4:    am__tar_='find "$tardir" -print | cpio -o -H $1 -L'
aclocal.m4:    am__untar='cpio -i -H $1 -d'
aclocal.m4:    am__tar=false
aclocal.m4:    am__tar_=false
aclocal.m4:    am__untar=false
aclocal.m4:  # If the value was cached, stop now.  We just wanted to have am__tar
aclocal.m4:  # and am__untar set.
aclocal.m4:  test -n "${am_cv_prog_tar_$1}" && break
aclocal.m4:  # tar/untar a dummy directory, and stop if the command works
aclocal.m4:  AM_RUN_LOG([tardir=conftest.dir && eval $am__tar_ >conftest.tar])
aclocal.m4:  if test -s conftest.tar; then
aclocal.m4:    AM_RUN_LOG([$am__untar <conftest.tar])
aclocal.m4:AC_CACHE_VAL([am_cv_prog_tar_$1], [am_cv_prog_tar_$1=$_am_tool])
aclocal.m4:AC_MSG_RESULT([$am_cv_prog_tar_$1])])
aclocal.m4:AC_SUBST([am__tar])
aclocal.m4:AC_SUBST([am__untar])
aclocal.m4:m4_include([m4/ltsugar.m4])
